			  			Ok, since i am too lazy to actually click on the voting links and such, I created a perl program to do my bidding. If you don't have perl, tough :-P If you do have perl, be sure you have lwp installed. On gentoo u just say &quot;emerge libwww-perl&quot;.<br />
Any event here's the code, sorry i could put it up on some site, my asianblvd.com site went b0rk3d.<br />
<br />
#!/usr/bin/perl<br />
#Autovoter.pl - by zbyte64<br />
use HTTP::Cookies;<br />
use HTTP::Request::Common qw(POST);<br />
use LWP::UserAgent;<br />
use Time::HiRes;<br />
<br />
$IDLE_TIME = 60*60*24;<br />
$PATH = 'http://www.mpogd.com/';<br />
$GAMDE_ID = 500;<br />
<br />
#set up cookies<br />
my $cookie_jar = HTTP::Cookies-&gt;new();<br />
#link browser to the cookies<br />
my $browser = LWP::UserAgent-&gt;new;<br />
$browser-&gt;cookie_jar( $cookie_jar );<br />
$browser-&gt;agent(&quot;Mozilla/4.0&quot;);<br />
<br />
print &quot;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;n&quot;;<br />
my $time_to_idle;<br />
while (1==1)<br />
{<br />
	$time_to_idle = time();<br />
	print &quot;Voting...n&quot;;<br />
	mpogd();<br />
	mpog();<br />
	onrpg();<br />
	ozgamers();<br />
	dog();<br />
	print &quot;Votedn&quot;;<br />
	$time_to_idle = $IDLE_TIME - (time() - $time_to_idle);<br />
	if ($time_to_idle &lt; 0 ) {<br />
		$time_to_idle = 0;<br />
	}<br />
	print 'Idling for '.($time_to_idle/60/60).&quot; hoursn&quot;;<br />
	print &quot;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;n&quot;;<br />
	sleep($time_to_idle);<br />
}<br />
<br />
sub mpogd {<br />
	print &quot;Mpogd.comn&quot;;<br />
	my $formpost = HTTP::Request-&gt;new(POST =&gt; 'http://www.mpogd.com/gotm/vote.asp');<br />
	$formpost-&gt;content_type('application/x-www-form-urlencoded');<br />
	$formpost-&gt;referer('http://www.mpogd.com/');<br />
	my $res = $browser-&gt;request($formpost);<br />
	print $res-&gt;content;<br />
	$formpost = HTTP::Request-&gt;new(POST =&gt; 'http://www.mpogd.com/gotm/vote.asp');<br />
	$formpost-&gt;content_type('application/x-www-form-urlencoded');<br />
	$formpost-&gt;referer('http://www.mpogd.com/gotm/vote.asp');<br />
	$formpost-&gt;content( &quot;gameid=500&amp;stage=vote&quot;);<br />
	my $res = $browser-&gt;request($formpost);<br />
	print $res-&gt;content;<br />
}<br />
<br />
sub mpog {<br />
	print &quot;Gamesites.comn&quot;;<br />
	my $formpost = HTTP::Request-&gt;new(GET =&gt; 'http://www.gamesites200.com/mpog/vote.php?id=1310');<br />
	$formpost-&gt;content_type('application/x-www-form-urlencoded');<br />
	$formpost-&gt;referer('http://www.archspace.org/');<br />
	my $res = $browser-&gt;request($formpost);<br />
	my $first = index $res-&gt;content, 'name=&quot;PHPSESSID&quot; value=&quot;';<br />
	$first += 24;<br />
	my $second = index $res-&gt;content, '&quot;', $first;<br />
	my $sessionid = substr($res-&gt;content, $first, $second-$first);<br />
	$formpost = HTTP::Request-&gt;new(POST =&gt; 'http://www.gamesites200.com/mpog/vote.php?id=1310');<br />
	$formpost-&gt;content( &quot;PHPSESSID=&quot;.$sessionid);<br />
	$formpost-&gt;content_type('application/x-www-form-urlencoded');<br />
	$formpost-&gt;referer('http://www.gamesites200.com/mpog/vote.php');<br />
	$res = $browser-&gt;request($formpost);<br />
	print $res-&gt;content;<br />
}<br />
<br />
sub onrpg {<br />
	print &quot;onrpg.comn&quot;;<br />
	my $formpost = HTTP::Request-&gt;new(GET =&gt; 'http://top50.onrpg.com/in.php?id=231');<br />
	$formpost-&gt;content_type('application/x-www-form-urlencoded');<br />
	$formpost-&gt;referer('http://www.archspace.org/');<br />
	my $res = $browser-&gt;request($formpost);<br />
	my $first = index $res-&gt;content, 'http://top50.onrpg.com/in.php?id=231&amp;session_id=';<br />
	$first += 48;<br />
	my $second = index $res-&gt;content, '&quot;', $first;<br />
	my $sessionid = substr($res-&gt;content, $first, $second-$first);<br />
	$formpost = HTTP::Request-&gt;new(GET =&gt; 'http://top50.onrpg.com/in.php?id=231&amp;session_id='.$sessionid);<br />
	$formpost-&gt;content_type('application/x-www-form-urlencoded');<br />
	$formpost-&gt;referer('http://top50.onrpg.com/in.php');<br />
	$res = $browser-&gt;request($formpost);<br />
	print $res-&gt;content;<br />
}<br />
<br />
sub ozgamers {<br />
	print &quot;ozgamers.comn&quot;;<br />
	my $formpost = HTTP::Request-&gt;new(POST =&gt; 'http://www.oz-games200.com/in.php?gameid=126');<br />
	$formpost-&gt;content_type('application/x-www-form-urlencoded');<br />
	$formpost-&gt;referer('http://www.archspace.org/');<br />
	my $res = $browser-&gt;request($formpost);<br />
	my $first = index $res-&gt;content, 'javascript:location=&quot;';<br />
	$first += 21;<br />
	my $second = index $res-&gt;content, '&quot;', $first;<br />
	my $url = substr($res-&gt;content, $first, $second-$first);<br />
	$formpost = HTTP::Request-&gt;new(GET =&gt; 'http://www.oz-games200.com/'.$url);<br />
	$formpost-&gt;content_type('application/x-www-form-urlencoded');<br />
	$formpost-&gt;referer('http://www.oz-games200.com/in.php');<br />
	$res = $browser-&gt;request($formpost);<br />
	print $res-&gt;content;<br />
}<br />
<br />
sub dog {<br />
	print &quot;dog.comn&quot;;<br />
	my $formpost = HTTP::Request-&gt;new(POST =&gt; 'http://www.directoryofgames.com/main.php?view=topgames&amp;action=vote&amp;v_tgame=214');<br />
	$formpost-&gt;content_type('application/x-www-form-urlencoded');<br />
	$formpost-&gt;referer('http://www.archspace.org/');<br />
	my $res = $browser-&gt;request($formpost);<br />
	my $first = index $res-&gt;content, 'name=confii value=&quot;';<br />
	$first += 19;<br />
	my $second = index $res-&gt;content, '&quot;', $first;<br />
	my $confii = substr($res-&gt;content, $first, $second-$first);<br />
	$formpost = HTTP::Request-&gt;new(POST =&gt; 'http://www.directoryofgames.com/main.php?view=topgames&amp;action=vote');<br />
	$formpost-&gt;content_type('application/x-www-form-urlencoded');<br />
	$formpost-&gt;referer('http://www.directoryofgames.com/main.php');<br />
	$formpost-&gt;content( &quot;votedef=&amp;v_tgame=214&amp;confii=&quot;.$confii);<br />
	$res = $browser-&gt;request($formpost);<br />
	print $res-&gt;content;<br />
}						<P align=right> </P>
